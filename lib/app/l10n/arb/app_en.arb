{
    "appName": "Diabetapp",
    "@appName": {
        "description": "Name of the app"
    },
    "nutrientsAdderCalculateButton": "Calculate",
    "@nutrientsAdderCalculateButton": {
        "description": "Text that submits the calculation of insuline and sugar"
    },
    "nutrientsActiveInsulineText": "Active Insuline",
    "@nutrientsActiveInsulineText": {
        "description": "Title for the active insuline text field"
    },
    "nutrientsCurrentSugarText": "Current Sugar",
    "@nutrientsCurrentSugarText": {
        "description": "Title for the current sugar text field"
    },
    "nutrientsYourMealText": "Your Meal",
    "@nutrientsYourMealText": {
        "description": "Title for the your meal text field"
    },
    "nutrientsResultGreatShape": "You are in great shape!",
    "@nutrientsResultGreatShape": {
        "description": "Text for result when user has to do anything"
    },
    "nutrientsResultHaveToEat": "You have to eat {amount} grams of carbs",
    "@nutrientsResultHaveToEat": {
        "description": "Text when user has to eat",
        "placeholders": {
            "amount": {
                "type": "String",
                "example": "0.3"
            }
        }
    },
    "nutrientsResultHaveToAdd": "You have to add {amount} of insuline",
    "@nutrientsResultHaveToAdd": {
        "description": "Text when user has to add insuline",
        "placeholders": {
            "amount": {
                "type": "String",
                "example": "0.3"
            }
        }
    },
    "nutrientsRecipeDescription": "Your meal has {quantity} grams of sugar | ",
    "@nutrientsRecipeDescription": {
        "description": "Amount of sugar the recipe has",
        "placeholders": {
            "quantity": {
                "type": "String",
                "example": "110"
            }
        }
    }
